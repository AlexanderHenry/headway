<h1></h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Role(s)</th>
      <th>Email</th>
    </tr>
  <% @users.each do |user| %>
    <tr>
      <td><% User.first_name %> <% User.last_name %></td>
      <td></td>
      <td><% User.email %></td>
      <td><% if current_user.admin? && !current_user?(user) %>
        <%= link_to "delete", user, method: :delete %>
        <% end %></td>
    </tr>
  <% end %>
  </table>
